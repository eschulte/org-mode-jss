@Article{Lenth:2007:SLP,
  author =       "Russell V. Lenth and S{\o}ren H{\o}jsgaard",
  title =        "\pkg{SASweave}: Literate Programming Using
                  \proglang{SAS}",
  journal =      j-J-STAT-SOFT,
  volume =       19,
  number =       8,
  pages =        "1--20",
  month =        may,
  year =         2007,
  CODEN =        "JSSOBK",
  ISSN =         "1548-7660",
  bibdate =      "Fri Jul 4 10:54:15 MDT 2008",
  URL =          "http://www.jstatsoft.org/v19/i08",
  abstract =     "\pkg{SASweave} is a collection of scripts that allow one
                  to embed SAS code into a {\LaTeX} document, and
                  automatically incorporate the results as
                  well. \pkg{SASweave} is patterned after \pkg{Sweave}, which does
                  the same thing for code written in \proglang{R}. In fact, a
                  document may contain both \proglang{SAS} and \proglang{R} code. Besides
                  the convenience of being able to easily incorporate
                  \proglang{SAS} examples in a document, \pkg{SASweave} facilitates the
                  concept of ``literate programming'': having code,
                  documentation, and results packaged together. Among
                  other things, this helps to ensure that the \proglang{SAS}
                  output in the document is in concordance with the
                  code.",
  acknowledgement =ack-nhfb,
  pubdates =     "Submitted 2007-02-14; Accepted 2007-05-27",
}

